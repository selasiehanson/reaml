{
  "name": "reaml",
  "version": "0.12.1",
  "license": "MIT",
  "files": [
    "bsconfig.json",
    "examples/*.ml",
    "ppx/bin/*.exe",
    "ppx/copy.js",
    "/ppx.exe",
    "src/*.ml"
  ],
  "scripts": {
    "postinstall": "cd ppx && node copy.js",
    "start": "npm-run-all -s build:rake build:bsb -p start:bsb start:parcel",
    "start:bsb": "bsb -make-world -w",
    "start:parcel": "parcel -d .cache/dev examples/index.html",
    "build": "run-s clean build:rake build:bsb test build:parcel",
    "build:rake": "rake && cd examples && rake",
    "build:bsb": "bsb -make-world",
    "build:parcel": "parcel build --public-url ./ --experimental-scope-hoisting examples/index.html",
    "test": "jest",
    "deploy": "run-s build deploy:netlify",
    "deploy:netlify": "netlify deploy --dir dist --prod --open --site 5d641476-5eb1-4c3b-b7c5-3aea4e66bc0d",
    "clean": "rimraf .cache dist && bsb -clean-world",
    "release": "make release"
  },
  "dependencies": {
    "bs-webapi": "^0.15.7"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.9",
    "bs-platform": "^7.1.1",
    "highlight.js": "^9.18.1",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "preact": "^10.3.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "jest": {
    "testMatch": [
      "**/lib/js/tests/**/*.bs.js"
    ]
  }
}
